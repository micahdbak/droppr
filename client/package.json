{
  "name": "droppr",
  "version": "0.1.0",
  "description": "Droppr.",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.24.3",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.24.3",
    "babel-loader": "^9.1.3",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "serve": "^14.2.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "format-workers": "prettier --write \"workers/**/*.js\"",
    "prebuild-workers": "npm run format-workers",
    "build-workers": "webpack --mode production",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "prebuild": "npm run format",
    "build": "react-scripts build",
    "postbuild": "npm run build-workers",
    "start": "react-scripts start",
    "serve": "serve build"
  }
