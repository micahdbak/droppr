{
  "name": "droppr-embeds",
  "version": "0.1.0",
  "description": "Peer-to-peer file transfer service.",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "babel-loader": "^9.1.3",
    "prettier": "^3.2.5",
    "serve": "^14.2.1",
    "webpack": "^5.91.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
